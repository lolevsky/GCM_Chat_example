<?xml version="1.0" encoding="utf-8" standalone="no"?><web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.5" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
	<servlet>
		<servlet-name>GCM_Save_Client</servlet-name>
		<servlet-class>com.iapplize.gcm.test.server.GCM_Save_Client_Servlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>GCM_Send_Message</servlet-name>
		<servlet-class>com.iapplize.gcm.test.server.GCM_Send_Message_Servlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>GCM_Get_User_List</servlet-name>
		<servlet-class>com.iapplize.gcm.test.server.GCM_Get_User_List_Servlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>GCM_Get_Message</servlet-name>
		<servlet-class>com.iapplize.gcm.test.server.GCM_Get_Message_Servlet</servlet-class>
	</servlet>
	
	
	<servlet-mapping>
		<servlet-name>GCM_Get_Message</servlet-name>
		<url-pattern>/gcm_get_Message</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>GCM_Save_Client</servlet-name>
		<url-pattern>/gcm_save_client</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>GCM_Send_Message</servlet-name>
		<url-pattern>/gcm_send_message</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>GCM_Get_User_List</servlet-name>
		<url-pattern>/gcm_get_user_list</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
 <servlet>
  <servlet-name>SystemServiceServlet</servlet-name>
  <servlet-class>com.google.api.server.spi.SystemServiceServlet</servlet-class>
  <init-param>
   <param-name>services</param-name>
   <param-value/>
  </init-param>
 </servlet>
 <servlet-mapping>
  <servlet-name>SystemServiceServlet</servlet-name>
  <url-pattern>/_ah/spi/*</url-pattern>
 </servlet-mapping>
</web-app>